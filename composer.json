{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "appzcoder/lumen-routes-list": "dev-master",
        "phpunit/php-timer": "~1.0",
        "lucadegasperi/oauth2-server-laravel": "~5.0",
        "barryvdh/laravel-cors": "0.7.x",
        "lahaxearnaud/clockwork": "dev-master",
        "illuminate/mail": "^5.1",
        "jeremeamia/SuperClosure": "^2.1",
        "rcrowe/twigbridge": "0.9.*",
        "league/oauth2-google": "^1.0",
        "duncan3dc/fork-helper": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "symfony/css-selector": "2.7.3",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "@stable",
        "fabpot/php-cs-fixer": "@stable",
        "sensiolabs/security-checker": "@stable",
        "apigen/apigen": "~4.1",
        "lahaxearnaud/clockwork-cli": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },

    "post-install-cmd": [
        "php artisan assets:update",
        "bower install",
        "npm install"
    ],
    "post-update-cmd": [
        "php artisan assets:update",
        "bower install",
        "npm install"
    ]
}
